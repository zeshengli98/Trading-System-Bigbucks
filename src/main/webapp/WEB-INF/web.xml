<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <display-name>On-line stock trading system</display-name>
    <servlet>
        <display-name>com.ibm.security.appscan.bigbucks.servlet.LoginServlet2</display-name>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.ibm.security.appscan.bigbucks.servlet.LoginServlet2</servlet-class>
    </servlet>

    <servlet>
        <display-name>com.ibm.security.appscan.bigbucks.servlet.SignUpServlet</display-name>
        <servlet-name>SignUpServlet</servlet-name>
        <servlet-class>com.ibm.security.appscan.bigbucks.servlet.SignUpServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/dologin</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>SignUpServlet</servlet-name>
        <url-pattern>/dosignup</url-pattern>
    </servlet-mapping>
</web-app>